<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
  	<servlet-name>addview</servlet-name>
  	<display-name>addview</display-name>
  	<description></description>
  	<servlet-class>org.example.addview</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Controller</servlet-name>
  	<display-name>Controller</display-name>
  	<description></description>
  	<servlet-class>org.example.Controller</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>student</servlet-name>
  	<display-name>student</display-name>
  	<description></description>
  	<servlet-class>org.example.student</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>addview</servlet-name>
  	<url-pattern>/addview.view</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Controller</servlet-name>
  	<url-pattern>/controller.do</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>student</servlet-name>
  	<url-pattern>/student.view</url-pattern>
  </servlet-mapping>
 <welcome-file-list>
  	<welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>
